<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    <!--Favicon-->
    <link rel="icon" type="image/png" sizes="50x50" href="~/Untitled design.png" />


    <!--CSS Stylesheets-->
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />-->
    <!--<link href="~/cubeportfolio/css/cubeportfolio.css" rel="stylesheet" />-->
    <link href="~/css/plugins/plugins.css" rel="stylesheet" />
    <link href="~/css/animate.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

    <!--Content Security Header -->
    <!--<meta http-equiv="Content-Security-Policy"
          content="default-src 'self'; child-src 'none'; script-src 'self'; img-src 'self'; style-src 'self' 'unsafe-inline';" />-->

    <!--Font Awesome Icons-->
    <script src="https://kit.fontawesome.com/2473e98242.js" crossorigin="anonymous"></script>

</head>
<body>
    <!--Preloader-->
    <div id="preloader">
        <div id="preloader-inner"></div>
    </div>



    <!--Navigation Menu-->
    <header>
        <!--Top Bar-->
        <div class="top-bar clearfix dark">
            <div class="container">
                <div class="float-sm-right">
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </div>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark nav-stick-top">

            <!--<nav class="navbar navbar-expand-lg navbar-light navbar-transparent bg-faded nav-sticky nav-stick-top">-->
            <div class="container">

                <a class="navbar-brand text-light" asp-area="" asp-controller="Home" asp-action="Index">Utah Collisions Analysis</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Summary">Database</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Analytics">Analytics</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Calculator">Calculator</a>
                        </li>
                    </ul>
                    <!--<partial name="_LoginPartial" />-->
                </div>
            </div>
        </nav>
    </header>

    @RenderBody()

    <partial name="_CookieConsentPartial" />

    <!--Footer-->
    <div class="footer-bottomAlt">
        <div class="container">
            <div class="row">
                <div class="col-lg-7">
                    <div class="clearfix">
                        <a href="#" class="social-icon-sm si-dark si-facebook si-dark-round">
                            <i class="fa fa-facebook"></i>
                            <i class="fa fa-facebook"></i>
                        </a>
                        <a href="#" class="social-icon-sm si-dark si-twitter si-dark-round">
                            <i class="fa fa-twitter"></i>
                            <i class="fa fa-twitter"></i>
                        </a>
                        <a href="#" class="social-icon-sm si-dark si-g-plus si-dark-round">
                            <i class="fa fa-google-plus"></i>
                            <i class="fa fa-google-plus"></i>
                        </a>
                        <a href="#" class="social-icon-sm si-dark si-skype si-dark-round">
                            <i class="fa fa-skype"></i>
                            <i class="fa fa-skype"></i>
                        </a>
                        <a href="#" class="social-icon-sm si-dark si-linkedin si-dark-round">
                            <i class="fa fa-linkedin"></i>
                            <i class="fa fa-linkedin"></i>
                        </a>
                    </div>
                </div>
                <div class="col-lg-5">
                    <span class="text-light">&copy; Copyright 2022. <a asp-controller="Home" asp-action="Privacy">Privacy</a></span>
                </div>
            </div>
        </div>
    </div>
    <!--back to top-->
    <a href="#" class="back-to-top hidden-xs-down" id="back-to-top"><i class="ti-angle-up"></i></a>

    <!--Start of Javascript-->
    <!--<script src="~/cubeportfolio/js/jquery.cubeportfolio.js"></script>-->
    <script src="~/js/plugins/plugins.js"></script>
    <script src="~/js/assan.custom.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/classie.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
